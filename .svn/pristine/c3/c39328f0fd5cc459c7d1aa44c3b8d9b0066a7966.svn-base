import { restartApiServerApi, } from '../config/api';
import HttpUtil from '../utils/httpUtil';

/**
 * 重启API服务
 * @returns {*}
 */
export const restartApiServer = () => {
  return HttpUtil.send(Object.assign({
    encode: false,
  }, restartApiServerApi));
};
